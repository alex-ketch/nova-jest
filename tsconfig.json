{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "rootDir": ".",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["../DefinitelyTyped/types/nova-editor"], // FIXME
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  }
}
